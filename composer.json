{
    "name": "tax16/system-check-bundle",
    "description": "Check availability of system",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.2",
        "symfony/http-client": "^5.2",
        "doctrine/orm": "^3.2",
        "ext-redis": "*",
        "php-amqplib/php-amqplib": "^3.0.0",
        "symfony/twig-bundle": "^5.4",
        "symfony/asset": "^5.4"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3 || ^3.0",
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "codeception/module-phpbrowser": "^2.0",
        "phpro/grumphp-shim": "^2.8",
        "phpstan/phpstan": "^1.12",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan-symfony": "^1.4",
        "composer/composer": "*"
    },
    "autoload": {
        "psr-4": {
            "Tax16\\SystemCheckBundle\\": "src/"
        },
        "psr-0": {
            "InstallConfig": "Composer/"
        }
    },
    "extra": {
        "class": "Installer",
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tahiana RAKOTONIRINA",
            "email": "tahiana0@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
