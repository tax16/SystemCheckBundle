{
    "name": "tax16/system-check-bundle",
    "description": "Check availability of system",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/http-client": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "doctrine/orm": "^2.5 || 3.*",
        "ext-redis": "*",
        "ruflin/elastica": "^7.2",
        "php-amqplib/php-amqplib": "^2.9",
        "symfony/twig-bundle": "^3.4 || ^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/asset": "^3.4 || ^4.4 || ^5.0 || ^6.0 || ^7.0",
        "ext-amqp": "*"
    },
    "require-dev": {
        "codeception/codeception": "^2.5 || ^4.0 || ^7.0",
        "codeception/module-asserts": "^1.3 || ^2.0",
        "doctrine/doctrine-bundle": "^1.10 || ^2.0",
        "symfony/browser-kit": "^3.4 || ^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/css-selector": "^3.4 || ^4.4 || ^5.0 || ^6.0 || ^7.0",
        "codeception/module-phpbrowser": "^1.0 || ^2.0",
        "phpro/grumphp-shim": "^v1.10 || ^2.8",
        "phpstan/phpstan": "^0.12 || ^1.0",
        "friendsofphp/php-cs-fixer": "^2.18 || ^3.0",
        "phpstan/phpstan-symfony": "^0.12 || ^1.0",
        "composer/composer": "*"
    },
    "autoload": {
        "psr-4": {
            "Tax16\\SystemCheckBundle\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tahiana RAKOTONIRINA",
            "email": "tahiana0@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
